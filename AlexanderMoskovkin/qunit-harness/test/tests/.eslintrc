{
  "env": {
    "browser": true,
    "jquery": true,
    "node": false
  },
  "rules": {
    "no-eval": 0,
    "max-nested-callbacks": 0,
    "no-script-url": 0
  },
  "globals": {
    /* QUnit variables */
    "QUnit": true,
    "ok": true,
    "notOk": true,
    "strictEqual": true,
    "notEqual": true,
    "deepEqual": true,
    "test": true,
    "asyncTest": true,
    "throws": true,
    "eval": true,
    "expect": true,
    "start": true,
    "module": true
  }
}
